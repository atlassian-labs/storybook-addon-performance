{
  "name": "storybook-addon-performance",
  "version": "0.15.1",
  "description": "A storybook addon to help better understand and debug performance for React components",
  "keywords": [
    "addon",
    "storybook",
    "performance",
    "storybook-addon",
    "test",
    "popular"
  ],
  "main": "dist/cjs/index.js",
  "types": "dist/esm/index.d.ts",
  "module": "dist/esm/index.js",
  "author": "Alex Reardon <areardon@atlassian.com>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/atlassian-labs/storybook-addon-performance.git"
  },
  "files": [
    "/dist",
    "/src",
    "register.js"
  ],
  "dependencies": {
    "@storybook/addons": "^6.1.11",
    "@storybook/api": "^6.1.11",
    "@storybook/channels": "^6.1.11",
    "@storybook/components": "^6.1.0",
    "@storybook/core-events": "^6.1.11",
    "@storybook/theming": "^6.1.11",
    "@testing-library/dom": "^7.22.0",
    "@testing-library/jest-dom": "^5.11.2",
    "@xstate/react": "1.0.0-rc.3",
    "gzip-js": "^0.3.2",
    "styled-components": "^5.1.1",
    "tiny-invariant": "^1.1.0",
    "xstate": "^4.11.0"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.5",
    "@storybook/react": "6.1.11",
    "@testing-library/react": "^10.4.8",
    "@types/gzip-js": "^0.3.1",
    "@types/jest": "^26.0.9",
    "@types/react": "^16.9.44",
    "@types/react-dom": "^16.9.8",
    "@types/react-select": "^3.0.16",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "babel-loader": "^8.1.0",
    "cypress": "^4.12.1",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react-hooks": "^4.0.8",
    "jest": "^26.2.2",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-select": "^3.1.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.1.4",
    "ts-loader": "^8.0.2",
    "tslib": "^2.0.1",
    "typescript": "^3.9.7"
  },
  "config": {
    "prettier_target": "src/**/*.{ts,tsx,js,jsx,md,json} test/**/*.{ts,tsx,js,jsx,md,json} examples/**/*.{ts,tsx,js,jsx,md,json}"
  },
  "bin": {
    "sb-perf": "dist/cli/index.js"
  },
  "scripts": {
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "storybook:dev": "start-storybook --port 9003",
    "storybook:build": "yarn build && build-storybook",
    "typescript:check": "tsc --noEmit",
    "typescript:watch": "tsc --watch",
    "typescript:build": "tsc && tsc --build tsconfig.cjs.json && tsc --build tsconfig.cli.json && chmod +x dist/cli/index.js",
    "prettier:check": "prettier --debug-check $npm_package_config_prettier_target",
    "prettier:write": "prettier --write $npm_package_config_prettier_target",
    "eslint:check": "eslint $npm_package_config_prettier_target --max-warnings=0",
    "validate": "yarn typescript:check && yarn prettier:check && yarn eslint:check",
    "dev": "yarn typescript:watch & yarn storybook:dev",
    "test": "jest",
    "build:clean": "rimraf dist",
    "build": "yarn build:clean && yarn typescript:build",
    "prepublishOnly": "yarn build"
  },
  "storybook": {
    "icon": "https://pbs.twimg.com/profile_images/1118574145724399616/oIyuo8uz_400x400.png",
    "displayName": "Performance"
  }
}
